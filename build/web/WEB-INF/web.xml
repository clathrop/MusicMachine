<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>serv</servlet-name>
        <servlet-class>serv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>musicservlet</servlet-name>
        <servlet-class>musicservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServ</servlet-name>
        <servlet-class>loginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerServ</servlet-name>
        <servlet-class>registerServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateServ</servlet-name>
        <servlet-class>updateServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>serv</servlet-name>
        <url-pattern>/serv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>musicservlet</servlet-name>
        <url-pattern>/musicservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServ</servlet-name>
        <url-pattern>/loginServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerServ</servlet-name>
        <url-pattern>/registerServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateServ</servlet-name>
        <url-pattern>/updateServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
